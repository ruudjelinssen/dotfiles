{
    "python.venvPath": "~/.virtualenvs",
    "python.linting.pylintArgs": ["--extension-pkg-whitelist=pygame"],
    "explorer.icon.enableNerdfont": true,
    "coc.preferences.formatOnSaveFiletypes": [
        "python",
        "rust"
    ],
    "diagnostic.errorSign": "",
    "diagnostic.infoSign": "",
    "diagnostic.warningSign": "⚠",
    "diagnostic.hintSign": ">>",
    "cSpell.enabledLanguageIds": [
        "markdown",
        "tex",
        "txt"
    ],
    "coc.preferences.rootPatterns": [
        "Cargo.toml"
    ],
    "languageserver": {
        "rust": {
            "command": "rust-analyzer",
            "filetypes": [
                "rust"
            ],
            "rootPatterns": [
                "Cargo.toml"
            ]
        }
    },
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    }
}
